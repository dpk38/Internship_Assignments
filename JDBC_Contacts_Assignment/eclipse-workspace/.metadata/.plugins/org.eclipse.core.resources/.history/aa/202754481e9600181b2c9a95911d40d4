	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int size = sc.nextInt();
		int leap = sc.nextInt();
		int[] arr = new int[size];
		boolean[] visited = new boolean[size];
		for (int i = 0; i < size; i++) {
			arr[i] = sc.nextInt();
		}
		
	}
}
